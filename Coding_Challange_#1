#include <stdio.h>

int main()
{
    float mass,height;
    float BMI;
    printf("Enter Mark's mass in kg: ");
    scanf("%f",&mass);
    printf("Enter Mark's height in meter: ");
    scanf("%f",&height);
    BMI = mass / (height * height);
    printf("BMI of Mark's:%f \n",BMI);
    
    printf("Enter John's mass in kg: ");
    scanf("%f",&mass);
    printf("Enter John's height in meter: ");
    scanf("%f",&height);
    BMI = mass / (height * height);
    printf("BMI of John's:%f \n",BMI);
    return 0;
}
